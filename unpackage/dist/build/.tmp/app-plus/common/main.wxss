@charset "UTF-8";
page {
  position: relative;
  height: 100%;
  width: 100%;
  font-family: PingFangSC-Regular;
  color: #131313;
}
/* 行为相关颜色 */
.color_primary {
  background: #007aff;
}
.color_success {
  background: #4cd964;
}
.color_warning {
  background: #f0ad4e;
}
.color_error {
  background: #dd524d;
}
.content {
  position: relative;
  height: 100%;
  width: 100%;
  overflow: hidden;
  background: #F5F5F5;
}
.container {
  position: relative;
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.bg-white {
  background: #fff;
}

