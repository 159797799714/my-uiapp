<view class="container data-v-1e32ffc1"><view class="topBar data-v-1e32ffc1" style="{{'padding-top:'+(statusBarHeight+'px')+';'}}"><text data-event-opts="{{[['tap',[['goBack',['$event']]]]]}}" class="iconfont data-v-1e32ffc1" bindtap="__e"></text><text data-event-opts="{{[['tap',[['addAction',['$event']]]]]}}" bindtap="__e" class="data-v-1e32ffc1">收货地址</text><text data-event-opts="{{[['tap',[['addAction',['$event']]]]]}}" class="iconfont data-v-1e32ffc1" bindtap="__e"></text></view><view class="content padding-30 border-box bg-white data-v-1e32ffc1"><block wx:for="{{addressList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['editAction',['$0'],[[['addressList','',index]]]]]]]}}" class="item data-v-1e32ffc1" bindtap="__e"><view class="row1 data-v-1e32ffc1"><text class="name data-v-1e32ffc1">{{item.name}}</text><text class="tel data-v-1e32ffc1">{{item.phone}}</text><block wx:if="{{item.address_id===default_id}}"><text class="tag data-v-1e32ffc1">默认</text></block><block wx:for="{{item.tags}}" wx:for-item="li" wx:for-index="num" wx:key="num"><text class="tag data-v-1e32ffc1">{{li}}</text></block></view><view class="address data-v-1e32ffc1">{{item.region.city+item.region.province+item.region.region+item.detail}}</view></view></block><block wx:if="{{addressList.length<1}}"><view class="blank font-99 t-center data-v-1e32ffc1"><text class="iconfont font-160 data-v-1e32ffc1"></text><view class="font-36 font-99 t-center data-v-1e32ffc1">暂无收货地址~</view></view></block></view><view data-event-opts="{{[['tap',[['addAction',['$event']]]]]}}" class="addBtn bg-black col-f t-center font-30 data-v-1e32ffc1" bindtap="__e">添加新地址</view></view>